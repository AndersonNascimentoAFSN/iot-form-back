{
  "name": "@andersonnascimentoafsn/back-nwl-pocket",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/http/server.js",
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch --env-file .env src/http/server.ts",
    "seed": "tsx --env-file .env src/db/seed.ts",
    "lint": "npx @biomejs/biome check",
    "lint:fix": "npx @biomejs/biome check --write .",
    "prepare": "husky",
    "deploy": "npm run build && npx drizzle-kit generate && npx drizzle-kit migrate",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:migrate": "npx drizzle-kit migrate",
    "drizzle:studio": "npx drizzle-kit studio",
    "seed:prod": "tsx --env-file .env.prod src/db/seed-education-levels.ts"
  },
  "keywords": [],
  "author": "Anderson Nascimento",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^22.5.4",
    "drizzle-kit": "^0.24.2",
    "husky": "^9.1.6",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "fastify": "^4.28.1",
    "fastify-type-provider-zod": "^2.0.0",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  }
}
